{
  "name": "@algo/server",
  "version": "0.0.0",
  "private": true,
  "author": "Nicolai Moraru <dot.nick.dot.messing@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "generate:prisma": "prisma generate",
    "generate:graphql:codegen": "graphql-codegen --config codegen.yml",
    "generate:graphql:format": "eslint src/generated --ext .ts --fix",
    "generate:graphql": "yarn generate:graphql:codegen && yarn generate:graphql:format",
    "generate": "yarn generate:prisma && yarn generate:graphql",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "dev:codegen": "nodemon --config nodemon.codegen.json",
    "dev:server": "nodemon --config nodemon.server.json",
    "dev": "concurrently -n codegen,\" server\" -c bgMagenta,bgBlue \"yarn:dev:codegen\" \"yarn:dev:server\"",
    "build": "tsc && cp -r src/graphql dist"
  },
  "devDependencies": {
    "@algo/eslint-config-eslint": "*",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-resolvers": "^2.7.6",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.14",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.6.2",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "concurrently": "^7.5.0",
    "eslint": "^8.27.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "graphql-subscriptions": "^2.0.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.9",
    "@prisma/client": "^4.6.0",
    "apollo-server-core": "^3.11.1",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.11.1",
    "aws-sdk": "^2.1249.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "google-auth-library": "^8.6.0",
    "graphql": "^16.6.0",
    "graphql-redis-subscriptions": "^2.5.0",
    "graphql-scalars": "^1.20.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.11.2",
    "mime-types": "^2.1.35",
    "node-fetch": "^2.6.7",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  }
}
